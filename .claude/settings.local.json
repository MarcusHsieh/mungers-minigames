{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "includeCoAuthoredBy": false,
  "env": {
    "MAX_THINKING_TOKENS": "31999",
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "32000"
  },
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(lsb_release:*)",
      "Bash(apt list:*)",
      "Bash(apt search:*)",
      "Bash(sudo apt:*)",
      "Bash(qmake:*)",
      "Bash(dpkg:*)",
      "Bash(qtchooser:*)",
      "Bash(pkg-config:*)",
      "Bash(find:*)",
      "Bash(chmod:*)",
      "Bash(g++:*)",
      "Bash(-exec:*)",
      "Bash(ls:*)",
      "Bash(./safe-airway:*)",
      "Bash(make:*)",
      "Bash(grep:*)",
      "Bash(cmake:*)",
      "Bash(timeout:*)",
      "Bash(realpath:*)"
    ],
    "deny": []
  }
}